#include <stdio.h>
#include <math.h>
#include <string.h>
 
int main(void)
{
int vetor[10], somaimpares = 0, contador;
 
for(contador = 0; contador < 10; contador++){
 
scanf("%d",&vetor[contador]);
 
if (vetor[contador] % 2 != 0){
somaimpares += vetor[contador];
}
}
 
printf("%d\n",somaimpares);
return 0;
}
